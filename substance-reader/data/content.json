    "heading_1": {
      "type": "heading",
      "id": "heading_1",
      "level": 2,
      "content": "A team of two"
    },
    "heading_2": {
      "type": "heading",
      "id": "heading_2",
      "level": 2,
      "content": "Priorities"
    },
    "text_3": {
      "type": "text",
      "id": "text_3",
      "content": "It has always been clear to us that the main challenge of Substance lies in creating a reliable web-based authoring experience. Before we could release a complete solution we would need to solve that problem at heart."
    },
